<!--index.wxml-->
<view class="container player">
  <view class="player-cover">
    <image src="{{cover}}"></image>
    <image class="fav" src="{{favicon}}" bindtap="toggleFavorite"></image>
  </view>
  <view class="player-audio">
    <audio  name="{{name}}" controls="true" author="{{author}}" src="{{src}}" id="myAudio" loop></audio>
  </view>
</view>
